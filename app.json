{
  "expo": {
    "name": "Planix",
    "slug": "Planix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Planix.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./PlanixFirebaseConfig.plist",
      "bundleIdentifier": "com.planix.planix"
    },
    "android": {
      "googleServicesFile": "./PlanixFirebaseServices.json",
      "package": "com.planix.planix",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/Planix.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  }
}
